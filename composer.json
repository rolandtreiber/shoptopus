{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "repositories": {
        "shoptopus/excel-import-export": {
            "type": "path",
            "url": "packages/shoptopus/excel-import-export",
            "options": {
                "symlink": true
            }
        }
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "bensampo/laravel-enum": "^3.4",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "google/apiclient": "^2.12",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.6",
        "laravel/framework": "^8.40",
        "laravel/passport": "^10.1",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.5",
        "owen-it/laravel-auditing": "^12.0",
        "s-ichikawa/laravel-sendgrid-driver": "^3.0",
        "sendgrid/sendgrid": "^7.9",
        "shoptopus/excel-import-export": "@dev",
        "spatie/laravel-permission": "^5.1",
        "spatie/laravel-searchable": "^1.10",
        "spatie/laravel-sluggable": "^3.3",
        "spatie/laravel-translatable": "^5.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "jasonmccreary/laravel-test-assertions": "^2.0",
        "laravel-shift/blueprint": "^1.24",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Seeders\\TestData\\": "database/seeders/test-data"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\Services\\Module\\": "app/Services/Module/",
            "App\\Facades\\": "app/Facades/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/passport"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
